{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Lark-Slack Connector",
    "version": "0.1.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "Copyright 2024 PLark-droid",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.plark.lark-slack-connector",
      "longDescription": "Bidirectional message bridge between Slack and Lark",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Lark-Slack Connector",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "systemTray": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": false,
      "menuOnLeftClick": true
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/PLark-droid/slack-to-lark-notifier/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDE2MjE4RjA1RkE4NzQ2OUUKUldTZVJvZjZCWThoRnMyU2p5R1krdjg4cHBHU1l6eUU1bHNSQ2NiTTkrQWE3NTRESUEwOE5jcTYK"
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Lark-Slack Connector",
        "width": 400,
        "visible": true,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false,
        "minHeight": 400,
        "minWidth": 350
      }
    ]
  }
}
